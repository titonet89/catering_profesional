# Node.js
node_modules/
npm-debug.log
npm-error.log
yarn-error.log
.npm

# Archivos de entorno (IMPORTANTE - DATOS PRIVADOS)
.env
.env.local
.env.*.local
.env.production

# IDE y editores
.vscode/
.idea/
*.swp
*.swo
*~
.DS_Store
.sublime-project
.sublime-workspace
*.code-workspace

# Logs
logs/
*.log

# Uploads locales (se regeneran en producción)
public/uploads/*
